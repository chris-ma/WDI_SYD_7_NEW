
<%= form_for @todos do |f| %>
  <p>
    <%= f.label :text %><br>
    <%= f.text_field :text %>
  </p>
 
  <p>
    <%= f.label :notes %><br>
    <%= f.text_area :notes %> 
  </p>

    <p>
    <%= f.check_box :completed %>
    <%= f.label :completed, "Done?" %> 
  </p>
 
  <p>
    <%= f.submit "Submit"%>
  </p>
<% end %>