<h1>New animal</h1>
 
<%= form_for :animal, url: animals_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :species %><br>
    <%= f.text_field :species %>
  </p>
 <p>
    <%= f.label :breed %><br>
    <%= f.text_field :breed %>
  </p>
   <p>
    <%= f.label :arrival_time %><br>
    <%= f.text_field :arrival_time %>
  </p>
   <p>
    <%= f.label :departure_time %><br>
    <%= f.text_field :departure_time %>
  </p>
    <p>
    <%= f.label :sex %><br>
    <%= f.text_field :sex %>
  </p>
  <p>
    <%= f.submit %>
  </p>
  <% end %>

<%= link_to 'Back', animals_path %>